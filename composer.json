{
    "name": "gam6itko/cbrf-api",
    "keywords": ["cbr.ru", "cbr", "api", "php", "russia", "banking", "credit organization info", "central bank of russian federation"],
    "description": "CBR.RU API for PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Strizhak",
            "email": "gam6itko@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-soap": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "7.*",
        "goetas-webservices/xsd2php-runtime": "^0.2.2",
        "goetas-webservices/xsd2php": "^0.3"
    },
    "suggest": {
        "jms/serializer": "Allows use of next level service classes to deserialize cbr-api responses to objects"
    },
    "autoload": {
        "psr-4": {
            "Gam6itko\\Cbrf\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
